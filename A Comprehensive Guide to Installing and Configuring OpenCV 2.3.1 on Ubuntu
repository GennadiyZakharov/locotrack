k;position:absolute}* html ul.rMenu ul,* html ul.rMenu-hor ul,* html ul.sub-menu ul,* html ul.rMenu-ver ul,* html ul.rMenu-vRight ul,* html ul.rMenu-hRight ul.sub-menu ul,* html ul.rMenu-hRight ul.rMenu-ver ul,* html ul.rMenu-hRight ul{left:-10000px}* html ul.rMenu li.sfhover{z-index:999}* html ul.rMenu li.sfhover ul{left:auto}* html ul.rMenu li.sfhover ul ul,* html ul.rMenu li.sfhover ul ul ul{display:none}* html ul.rMenu li.sfhover ul,* html ul.rMenu li li.sfhover ul,* html ul.rMenu li li li.sfhover ul{display:block}* html ul.sub-menu li.sfhover ul,* html ul.rMenu-ver li.sfhover ul{left:60%}* html ul.rMenu-vRight li.sfhover ul,* html ul.rMenu-hRight ul.sub-menu li.sfhover ul* html ul.rMenu-hRight ul.rMenu-ver li.sfhover ul{left:-60%}* html ul.rMenu iframe{position:absolute;left:0;top:0;z-index:-1}* html ul.rMenu{margin-left:1px}* html ul.rMenu ul,* html ul.rMenu ul ul,* html ul.rMenu ul ul ul,* html ul.rMenu ul ul ul ul{margin-left:0}.clearfix:after{ content:".";display:block;height:0;clear:both;visibility:hidden}.clearfix{min-width:0;display:inline-block;display:block}* html .clearfix{height:1%;}.clearboth{clear:both;height:1%;font-size:1%;line-height:1%;display:block;padding:0;margin:0}code{font-size:120%;font-family:Consolas,"Lucida Console",Monaco,"Courier New",Courier,monospace}pre{font-size:85%;font-family:Consolas,"Lucida Console",Monaco,"Courier New",Courier,monospace}.post-bodycopy img{display:block;float:left;clear:both;}.excerptimg{margin:0 10px 0 0;display:block;float:left}div.post-headline h2 a:link,div.post-headline h2 a:visited,div.post-headline h2 a:active{color:#2970ff;_background:#ffffff}div.post-headline h2 a:hover{color:#aa0000;_background:#aa0000}div.post-footer a:link,div.post-footer a:visited,div.post-footer a:active{color:#2970ff;}div.post-footer a:hover{color:#aa0000;}.ozimgleftxxlarge{width:1000px;margin:0 1000px 10px 0;}.ozimgleftxlarge{width:800px;margin:0 1000px 10px 0;}.ozimgleftlarge{width:600px;margin:0 1000px 10px 0;}.ozimgleftmedium{width:400px;margin:0 1000px 10px 0;}.ozimgleftsmall{width:300px;margin:0 1000px 10px 0;}.ozimgleftxsmall{width:200px;margin:0 1000px 10px 0;}#menu1{background:#89CB11;padding:10px 0 10px 10px}#_menu1 ul{padding-left:5px;}#menu1 li{margin:0;padding:0 20px 0 20px;}._clearfix.rss-box a{padding:0 25px 0 25px }.clearfix.rss-box{padding-right:15px }#_menu1 .menu-item a:active,#menu1 .page_item a:active{}#_menu1 .menu-item a:link,#menu1 .page_item a:link{}#_menu1 .menu-item a:visited,#menu1 .page_item a:visited{}#_menu1 .menu-item a:hover,#_menu1 .page_item a:hover{ background-color:#ff0000; color:#333333;text-shadow:#000 1px 1px 1px;border-radius:10px;-moz-border-radius:10px; }div#menu1 ul.rMenu li.page_item,div#menu1 ul.rMenu li.menu_item{background-color:#89cb11;}div#menu1 ul.rMenu li.page_item a:hover,div#menu1 ul.rMenu li.menu_item a:hover{text-shadow:#000 1px 1px 1px;}div#menu1 ul.rMenu li.page_item,div#menu1 ul.rMenu li.menu_item{background-color:#89cb11;}div#menu1 ul.rMenu li.page_item.current_page_item a,div#menu1 ul.rMenu li.menu_item.current_menu_item a{background-color:#ffffff;color:#000000;border-radius:10px;-moz-border-radius:10px;}div#oz-read-more-button a{font-weight:normal;border-radius:5px;padding:7px;color:#ffffff;background-color:#2970ff}div#oz-read-more-button a:hover{text-decoration:none;text-shadow:#000 1px 1px 1px}_h1{font-size:34px;line-height:1.2;margin:0.3em 0 10px;}_h2{font-size:28px;line-height:1.3;margin:1em 0 .2em;}_h3{font-size:24px;line-height:1.3;margin:1em 0 .2em;}_h4{font-size:19px;margin:1.33em 0 .2em;}_h5{font-size:1.3em;margin:1.67em 0;font-weight:bold;}_h6{font-size:1.15em;margin:1.67em 0;font-weight:bold;}</style>
